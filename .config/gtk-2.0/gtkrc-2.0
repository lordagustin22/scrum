include "/home/utane/.gtkrc-2.0.mine"
gtk-theme-name="Gruvbox-Material-Dark"
gtk-icon-theme-name="Gruvbox-Material-Dark"
gtk-font-name="Roboto 11"
gtk-cursor-theme-name="Premium"
gtk-cursor-theme-size=18
