account niconecrofilo589@gmail.com
host smtp.gmail.com
port 587
from niconecrofilo589@gmail.com
user niconecrofilo589@gmail.com
passwordeval "pass niconecrofilo589@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/utane/.config/msmtp/msmtp.log
# tls_starttls

