macro index,pager i2 '<sync-mailbox><enter-command>source /home/utane/.config/mutt/accounts/aguscea18@gmail.com.muttrc<enter><change-folder>!<enter>;<check-stats>' "switch to aguscea18@gmail.com"
macro index,pager gd "<change-folder>=[Gmail]/Borradores<enter>" "go to drafts"
